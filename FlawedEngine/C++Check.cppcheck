    <max-ctu-depth>2</max-ctu-depth>
    <max-template-recursion>100</max-template-recursion>
    <vs-configurations>
        <config>Debug</config>
        <config>Release</config>
    </vs-configurations>
    <libraries>
        <library>opengl</library>
    </libraries>
    <safe-checks>
        <class-public/>
    </safe-checks>
</project>
